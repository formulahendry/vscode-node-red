{
    "name": "vscode-node-red",
    "displayName": "Node-RED",
    "description": "Flow-based programming for IoT (Internet of Things)",
    "version": "0.0.1",
    "publisher": "formulahendry",
    "icon": "logo.png",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "IoT",
        "Node-RED",
        "Raspberry Pi",
        "Arduino",
        "Android"
    ],
    "bugs": {
        "url": "https://github.com/formulahendry/vscode-node-red/issues",
        "email": "formulahendry@gmail.com"
    },
    "homepage": "https://github.com/formulahendry/vscode-node-red/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/formulahendry/vscode-node-red.git"
    },
    "activationEvents": [
        "*",
        "onCommand:node-red.open"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "node-red.open",
                "title": "Open",
                "category": "Node-RED"
            }
        ],
        "keybindings": [
            {
                "command": "node-red.open",
                "key": "ctrl+alt+x"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/express": "^4.11.1",
        "@types/get-port": "^3.2.0",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/node-red": "^0.17.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "applicationinsights": "^1.0.2",
        "express": "^4.16.3",
        "get-port": "^3.2.0",
        "node-red": "^0.18.4",
        "node-red-embedded-start": "^1.0.1"
    }
}
